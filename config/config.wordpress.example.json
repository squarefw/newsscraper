{
  "api": {
    "baseUrl": "http://localhost:3000/api",
    "apiKey": "your-api-key-here",
    "email": "your-email@example.com",
    "password": "your-password"
  },
  "wordpress": {
    "enabled": true,
    "baseUrl": "https://your-wordpress-site.com",
    "username": "your-wp-username",
    "password": "your-wp-application-password",
    "defaultStatus": "draft",
    "defaultCategory": "新闻"
  },
  "ai": {
    "enabled": true,
    "engine": "ollama",
    "ollama": {
      "baseUrl": "http://localhost:11434",
      "model": "llama3.1:8b",
      "timeout": 600000
    },
    "tasks": [
      "translate",
      "rewrite",
      "summarize",
      "extract_keywords",
      "categorize",
      "sentiment"
    ]
  },
  "filter": {
    "keywords": [
      "都柏林",
      "爱尔兰",
      "爱尔兰人",
      "都柏林",
      "爱尔兰新闻",
      "ireland",
      "irish",
      "dublin"
    ]
  },
  "testMode": true
}
